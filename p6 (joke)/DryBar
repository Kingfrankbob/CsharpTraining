//using System.IO;

namespace comedians
{
    public static class DryBar
    {
        public static void Comedy()
        {
            var count = 0;
            string all = File.ReadAllText(BeeMovie.txt);
            var words = all.Split(' ');
            foreach (var char in words)
            {
                count++;
            }
            Console.WriteLine("Total number of words is:" + count);
        }
    }
}